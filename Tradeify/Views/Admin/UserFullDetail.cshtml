

<div class="col-lg-12 col-md-12 col-sm-12 col-12 bg-dark">
    <div class="sv_question_pop float_left ">
        <h1 class="text-center text-white">
            @Model?.Name.ToUpper()

        </h1>

        <div class="search_alert_box float_left">
            <div class="change_field text-light ">
                <h1>
                    Email :&nbsp;&nbsp;  @Model?.Email

                </h1>
            </div>
            <div class="change_field text-light">
                <h1>
                    User Name :&nbsp;&nbsp; @Model?.UserName
                </h1>
            </div>
            <div class="change_field text-light">
                <h1>
                    Phone :&nbsp;&nbsp; @Model?.PhoneNumber
                </h1>

            </div>
            <div class="change_field text-light">
                <h1>
                    Gender :&nbsp;&nbsp;  @Model?.Gender?.Name
                </h1>
            </div>
            @*<div class="change_field text-light">
                <h1>
                Reffer Name :&nbsp;&nbsp; @Model.Refferrer.Name
                </h1>

            </div>*@
            <div class="change_field text-light">
                <h1>
                    Parent Name :&nbsp;&nbsp; @Model?.Parent?.Name
                </h1>

            </div>
            <div class="change_field text-light">
                <h1>
                    Date Registered :&nbsp;&nbsp;  @Model?.DateRegistered.ToString("D")
                </h1>

            </div>
            <div class="change_field text-light">
                <h1>
                    Registration Status :&nbsp;&nbsp;  @Model?.RegFeePaid
                </h1>

            </div>

        </div>

        <div class="question_sec float_left">
            <div class="cancel_wrapper">
                <a href="#" class="" data-dismiss="modal">cancel</a>
            </div>
        </div>

    </div>
</div>

<style>
    .change_field h1 {
        color: white;
    }
</style>                           